package ch02.sub.step3;

public class NanoStopWatch {
	// nanoseconds 도메인
	long startTime;
	long endTime;
	
	public void startNano() {
		startTime = System.nanoTime();
	}
	
	public void stopNano() {
		endTime = System.nanoTime();
	}
	
	public long getElapsedNanoTime() {
		return endTime - startTime;
	}
}
