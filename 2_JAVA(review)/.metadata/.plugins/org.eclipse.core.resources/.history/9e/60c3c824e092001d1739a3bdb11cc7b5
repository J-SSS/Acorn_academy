package ch05.ex04.case03;

import java.io.FileWriter;
import java.io.IOException;

public class FileWriterTest {
	public static void main(String[] args) {
		
		try (FileWriter fw = new FileWriter("writer.txt")) {
			fw.write('A'); // 문자 하나 출력
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
