package ch02.sub.step5;

public class MilliStopWatch extends StopWatch {
	
	@Override
	public void start() {
		startTime = System.currentTimeMillis();
	}
	
	@Override
	public void stop() {
		endTime = System.currentTimeMillis();
	}
	
	@Override
	public long getElapsedTime() {
		return endTime - startTime;
	}
}
